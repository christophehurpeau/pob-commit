{
  "name": "pob-commit",
  "version": "0.1.3",
  "description": "commit with ease",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "contributors": [
  ],
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/pob-commit.git",
  "homepage": "https://github.com/christophehurpeau/pob-commit",
  "main": "./index.js",
  "bin": {
    "commit": "./commit",
    "prepare-commit-msg": "./prepare-commit-msg"
  },
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "commit": "./commit",
    "lint": "eslint --ext .js,.jsx --fix src/",
    "prepublish": "ln -s ../../git-hooks/pre-commit .git/hooks/pre-commit 2>/dev/null || true ; ln -s ../../git-hooks/post-checkout .git/hooks/post-checkout 2>/dev/null || true ; ln -s ../../git-hooks/post-merge .git/hooks/post-merge 2>/dev/null || true ; ln -s ../../git-hooks/prepare-commit-msg .git/hooks/prepare-commit-msg 2>/dev/null || true",
    "preversion": "npm run lint && npm run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^1.2.1",
    "liftoff": "^2.3.0",
    "minimist-argv": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-config-pob": "^7.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "pob-babel": "^8.3.2",
    "pob-commit-karma": "./plugins/pob-commit-karma",
    "pob-release": "^2.1.2",
    "tcomb-forked": "^3.4.0"
  }
}
